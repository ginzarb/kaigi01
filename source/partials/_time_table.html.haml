%section.section.section--time-table
  %h2 タイムテーブル
  .time-table
    %table.time-table__table
      %tr
        %th.time-table__th
          12:00
        %td.time-table__td
          %h3.time-table__hl
            開場

      %tr
        %th.time-table__th
          13:00
        %td.time-table__td
          %h3.time-table__hl
            開幕のごあいさつ

      %tr
        %th.time-table__th
          13:05
        %td.time-table__td
          %h3.time-table__hl
            基調講演

          .time-table__speaker
            =image_tag('https://pbs.twimg.com/profile_images/1241352454/face_icon.jpg', class: 'time-table__speaker-image')
            .time-table__speaker-data
              .time-table__speaker-name 松田 明
              .time-table__speaker-id=link_to '@a_matsuda', 'https://github.com/amatsuda', target: '_blank'

      %tr
        %th.time-table__th
          13:50
        %td.time-table__td
          %h3.time-table__hl
            スポンサーLT

      %tr
        %th.time-table__th
          13:55
        %td.time-table__td
          %h3.time-table__hl
            ActiveSupport::Multibyte::Unicode::UnicodeDatabase を消したかった
          .time-table__speaker
            =image_tag('https://avatars3.githubusercontent.com/u/1796864?v=3&s=400', class: 'time-table__speaker-image')
            .time-table__speaker-data
              .time-table__speaker-name Fumiaki MATSUSHIMA
              .time-table__speaker-id=link_to '@mtsmfm', 'https://github.com/mtsmfm', target: '_blank'

      %tr
        %th.time-table__th
